{
  "name": "banka",
  "version": "1.0.0",
  "description": "Banka is a light-weight core banking application that powers banking operation on the go. With Banka you can create an account, make deposit, see your transaction overview and much more...",
  "main": "server/app.js",
  "scripts": {
    "lint": "eslint",
    "start": "node app.js",
    "build": "babel server -d server/dist",
    "start-dev": "nodemon --exec babel-node server/app.js",
    "test": "nyc --require babel-core/register mocha server/test/**.spec.js --exit -w",
    "test-single": "nyc --require babel-core/register mocha server/test/**.spec.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/encodedBicoding/banka.git"
  },
  "author": "Dominic Isioma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/encodedBicoding/banka/issues"
  },
  "homepage": "https://github.com/encodedBicoding/banka#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "jscover": "^1.0.0",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.5.1"
  }
}
